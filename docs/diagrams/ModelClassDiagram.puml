@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyProfPlan" as ReadOnlyProfPlan
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class ProfPlan
Class ModelManager
Class UserPrefs

Class UniqueTaskList
Class Task
Class Address
Class Email
Class Name
Class
Class Tag

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

ProfPlan .up.|> ReadOnlyProfPlan

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyProfPlan
ModelManager -left-> "1" ProfPlan
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

ProfPlan *--> "1" UniqueTaskList
UniqueTaskList --> "~* all" Task
Task *--> Name
Task *-->
Task *--> Email
Task *--> Address
Task *--> "*" Tag

Task -[hidden]up--> I
UniqueTaskList -[hidden]right-> I

Name -[hidden]right->
 -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Task
@enduml
